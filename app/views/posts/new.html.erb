<h1>画像投稿フォーム</h1>
<%= form_with model: @post, local:true do |f| %>
  <h4>画像</h4>
  <%= f.attachment_field :images_images, multiple: true %>
  <h4>会社名</h4>
  <%= f.text_field :company %>
  <h4>路線名</h4>
  <%= f.text_field :line %>
  <h4>形式</h4>
  <%= f.text_field :series %>
  <h4>編成</h4>
  <%= f.text_field :car %>
  <h4>場所</h4>
  <%= f.text_field :place %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  <h4>本文</h4>
  <%= f.text_area :body %>
  <h4>日付</h4>
  <%= f.text_field :date %>
  <%= f.submit '投稿' %>
<% end %>