<div class="container">
  <div class="row">
    <div class="col-md-6 offset-md-3 mb-3">
      <h1 class="mb-5 text-center">画像投稿フォーム</h1>
      <%= form_with model: @post, local:true do |f| %>
      <div class="container bg-light">
        <h4>画像</h4>
  <%= f.attachment_field :images_images, multiple: true %>
  <h4>会社名</h4>
  <%= f.text_field :company %>
  <h4>路線名</h4>
  <%= f.text_field :line %>
  <h4>形式</h4>
  <%= f.text_field :series %>
  <h4>編成</h4>
  <%= f.text_field :car %>
  <h4>場所</h4>
  <%= f.text_field :place %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  <h4>本文</h4>
  <%= f.text_area :body %>
  <h4>日付</h4>
  <%= f.text_field :date %>
  <%= f.submit '投稿' %>
<% end %>
      </div>
    </div>
  </div>
</div>